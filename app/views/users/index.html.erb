<h1>Listing users</h1>

<table>
  <tr>
    <th>First name</th>
    <th>Second name</th>
    <th>Home country</th>
    <th>Phone number</th>
    <th>Email address</th>
    <th>Price method</th>
    <th>Price category</th>
    <th>Currency</th>
    <th>Payment</th>
    <th>Reference number</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.first_name %></td>
    <td><%= user.second_name %></td>
    <td><%= user.home_country %></td>
    <td><%= user.phone_number %></td>
    <td><%= user.email_address %></td>
    <td><%= user.price_method %></td>
    <td><%= user.price_category %></td>
    <td><%= user.currency %></td>
    <td><%= user.payment %></td>
    <td><%= user.reference_number %></td>
  </tr>
<% end %>
</table>
